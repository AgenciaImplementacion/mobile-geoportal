{
  "name": "mobile-geoportal",
  "version": "0.0.1",
  "description": "Versión móvil del geoportal",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.config.js --display-modules",
    "live": "webpack-dev-server --progress --colors",
    "watch": "webpack --watch --colors",
    "pack": "npm run build && tar -czvf build.tar.gz build",
    "clean": "rm -rf build build.tar.gz"
  },
  "keyworks": [
    "geoportal",
    "sig",
    "gis"
  ],
  "author": "Agencia Implementación Swissphoto Incige",
  "license": "AGPLv3",
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.30.1",
    "jshint": "^2.9.5",
    "jshint-loader": "^0.8.4",
    "url-loader": "^0.5.9",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "chart.js": "^2.7.0",
    "color": "^1.0.3",
    "jquery": "^3.2.1",
    "materialize-css": "^0.100.2",
    "ol": "^4.3.4",
    "proj4": "^2.4.4"
  }
}
